<text>我的历史</text>
<view class="page-section">
    <view class="weui-cells weui-cells_after-title">
      <radio-group bindchange="radioChange">
        <label class="weui-check__label" wx:for="{{items}}" wx:key="{{item.value}}">
          <view class="weui-cell__hd">
            <radio value="{{item.value}}" checked="true"/>
          </view>
          <view class="weui-cell__bd">{{item.name}}</view>
        </label>
      </radio-group>
    </view>
  </view>
  <view class="image-list" wx:if="{{showdata.length > 0}}">
    <view class="item" wx:for="{{showdata}}" wx:key="key">
      <view>时间：{{item.time}}</view>
      <image src="{{item.fileID}}" bindtap="preview" data-src="{{item.path}}"></image>
    </view>
  </view>
  <view wx:if="{{showdata.length == 0}}">
    <dialog id='dialog' 
        title='提示' 
        content='查询结果为空' 
        cancelText='取消' 
        confirm='确定' 
        isShown="{{isShown}}"
        bind:cancelEvent="_cancelEvent" 
        bind:confirmEvent="_confirmEvent"/>
  </view>
